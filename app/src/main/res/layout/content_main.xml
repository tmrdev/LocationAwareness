<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="org.tmreynolds.humanproject.MainActivity"
    tools:showIn="@layout/activity_main">

    <Button
        android:id="@+id/distance_results_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dump Log"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <Button
        android:id="@+id/logout_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Logout"
        android:layout_toEndOf="@+id/distance_results_button" />

    <ScrollView
        android:id="@+id/distance_results_scroll"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/distance_results_button"
        android:layout_centerHorizontal="true">

        <TextView
            android:id="@+id/distance_results"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Test"
            android:layout_below="@+id/distance_results_button" />

        </ScrollView>


</RelativeLayout>
